{
    "manifest_version": 2,
    "name": "Extension Demo",
    "version": "1.0.1",
    "background":{
        "scripts": ["bg.js"]
    },
    "description": "Build my EECS1720 Lab1project Extension!",
    "icons": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
    },
    "browser_action": {
      "default_icon":{  
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"},
      "default_popup": "show.html"
    },
    "permissions": [
      "contextMenus",
      "storage",
      "default_popup",
      "activeTab"]
    
  }